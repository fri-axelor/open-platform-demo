<?xml version="1.0" encoding="UTF-8" standalone="yes"?>
<object-views xmlns="http://axelor.com/xml/ns/object-views" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
  xsi:schemaLocation="http://axelor.com/xml/ns/object-views http://axelor.com/xml/ns/object-views/object-views_5.4.xsd">

  <grid model="com.axelor.account.db.Move" title="Moves" name="move-grid" canNew="false" canEdit="false" canDelete="false">
    <field name="sequence"/>
    <field name="operationDate"/>
  </grid>

  <form model="com.axelor.account.db.Move" title="Moves" name="move-form" canNew="false" canEdit="false" canDelete="false">
    <panel name="mainPanel">
      <field name="sequence" css="label-bold large" colSpan="12"/>
    </panel>

    <panel-related field="moveLineList" grid="move-line-grid" form="move-line-form"/>

    <panel name="sidePanel" sidebar="true">
      <field name="operationDate"/>
    </panel>
  </form>

</object-views>
