<?xml version="1.0" encoding="UTF-8"?>
<domain-models xmlns="http://axelor.com/xml/ns/domain-models"
  xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
  xsi:schemaLocation="http://axelor.com/xml/ns/domain-models http://axelor.com/xml/ns/domain-models/domain-models_5.4.xsd">

  <module name="sale" package="com.axelor.sale.db"/>

  <entity name="SaleOrder">
    <date name="invoiceDate" copy="false"/>
    <date name="forecastDate"/>
    <many-to-one ref="com.axelor.invoice.db.Invoice" name="invoice" title="Invoice"/>
    <extra-code>
          <![CDATA[

           // ORDER STATUS
        public static final int STATUS_INVOICED = 4;
      ]]>
    </extra-code>
  </entity>

</domain-models>
